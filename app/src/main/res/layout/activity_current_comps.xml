<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">




    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        app:layout_constraintHeight_percent="0.5"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/bgna"
        tools:layout_editor_absoluteX="0dp" />
    <TextView
        android:id="@+id/textView"
        android:layout_width="272dp"
        android:layout_height="72dp"
        android:layout_marginEnd="65dp"
        android:text="All Complaints"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.878"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="98dp" />
    <ListView
        android:id="@+id/comp_list_view"
        android:layout_width="235dp"
        android:layout_height="260dp"
        android:backgroundTint="#000000"
        android:divider="@null"
        android:dividerHeight="0dp"
        android:foregroundTint="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.159"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.38"
        app:layout_constraintWidth_percent="0.9" >



    </ListView>

    <Button
        android:id="@+id/go_to_create_button"
        android:layout_width="197dp"
        android:layout_height="41dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/rounded_edittext_background"
        android:backgroundTint="#F1C644"
        android:text="Create New Complaint"
        app:cornerRadius="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/comp_list_view" />


